    "AGGLOMERATIVE_DISTANCE_THRESHOLD": 0.5,
    "EDGE_INFERENCE_MODEL": "gpt-4o-2024-05-13",
    "EDGE_INFERENCE_TEMPERATURE": 0.0,
    "EDGE_INFERENCE_BATCH_SIZE": 5,
