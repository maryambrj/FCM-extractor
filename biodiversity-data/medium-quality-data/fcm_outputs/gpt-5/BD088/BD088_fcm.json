    "POST_CLUSTERING_EMBEDDING_MODEL": "sentence-transformers/allenai-specter",
    "POST_CLUSTERING_MAX_MERGES_PER_CLUSTER": null,
    "POST_CLUSTERING_REQUIRE_MINIMUM_SIMILARITY": true,
    "ACO_MAX_ITERATIONS": 5,
